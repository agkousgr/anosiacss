<div class="well">
    <div class="cart-list">
        {% if cartItems|length > 0 %}
            {% for pr in cartItems %}
                <div class="media">
                    <div class="media-left"><a href="#">
                            <img class="media-object" src="{{ pr.imageUrl }}" alt="..."></a>
                    </div>
                    <div class="media-body">
                        <h5>{{ pr.name }}</h5>
                        {#<div class="star-rating">#}
                        {#<i class="fa fa-star"></i>#}
                        {#<i class="fa fa-star"></i><i class="fa fa-star"></i>#}
                        {#<i class="fa fa-star"></i><i class="fa fa-star-o"></i>#}
                        {#</div>#}
                        <div class="clearfix">
                            <div class="qty-box">
                                <input class="form-control stepper" type="number" value="1" min="1" max="10"/>
                            </div>
                            <div class="cart-item-price">{{ pr.webPrice }}â‚¬</div>
                        </div>
                        <button type="button" class="remove-item">
                            <i class="fa fa-times"></i>
                        </button>
                    </div>
                </div>
            {% endfor %}
        {% endif %}
    </div>
    <div class="cart-total clearfix">
        Cart Subtotal: <span>$747.00</span>
    </div>
    <div class="row btns">
        <div class="col-xs-10 col-xs-offset-1">
            <a href="#" class="btn btn-default btn-block">Checkout</a>
        </div>
    </div>
</div>